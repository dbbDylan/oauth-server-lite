{
	"logger": {
		"dir": "logs/",
		"level": "DEBUG",
		"keepHours": 24
	},
	"cas": "http://localhost:8444/cas/",
	"db": {
		"sqlite":"sqlite.db",
		"db_debug": false
	},
	"redis": {
		"dsn": "redis:6379",
		"max_idle": 5,
		"conn_timeout": 5,
		"read_timeout": 5,
		"write_timeout": 5,
		"password": ""
	},
	"redis_namespace": {
		"oauth": "oauth:",
		"cache": "cache:",
		"lock": "lock:",
		"fail": "fail:"
	},
	"http": {
		"listen": "0.0.0.0:8080",
		"manage_ip": ["127.0.0.1"],
		"x-api-key": "shanghai-edu",
		"session_options": {
			"path": "/",
			"domain": "open-oauth2playground:80",
			"max_age": 7200,
			"secure": false,
			"http_only": false
		},
		"max_multipart_memory": 100
	},
	"max_failed": 5,
	"failed_intervel": 300,
	"lock_time": 600,
	"access_token_expired": 7200,
	"old_access_token_expired": 300,
	"refresh_token_expired_day": 365,
	"code_expired": 300
}